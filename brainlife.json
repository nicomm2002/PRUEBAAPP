{
    "name": "PRUEBAAPP",
    "description": "Aplicación de prueba basada en MATLAB para Brainlife.io",
    "author": "Nicolas Martinez",
    "license": "MIT",
    "version": "1.0.0",
    "inputs": [
        {
            "id": "config",
            "type": "file",
            "description": "Archivo de configuración brainlife.json",
            "required": true
        },
        {
            "id": "input1",
            "type": "file",
            "description": "Archivo de entrada config.json"
        }
    ],
    "outputs": [
        {
            "id": "suma",
            "type": "file",
            "description": "Archivo de salida resultado.mat"
        }
    ],
    "dependencies": {
        "matlab": "R2021a"
    },
    "command": "matlab -batch \"run('main.m')\""
}
